{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "anlutro/l4-settings": "^0.5.0",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "barryvdh/laravel-snappy": "^0.3.3",
        "cviebrock/eloquent-sluggable": "^4.2",
        "dompdf/dompdf": "^0.8.0",
        "guzzlehttp/guzzle": "^6.2",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "intervention/image": "^2.3",
        "knplabs/knp-snappy": "^0.4.3",
        "laracasts/utilities": "^2.1",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "maatwebsite/excel": "^2.1",
        "mikehaertl/phpwkhtmltopdf": "^2.2",
        "milon/barcode": "^5.3",
        "nategood/httpful": "^0.2.20",
        "nesbot/carbon": "^1.22",
        "nicolaslopezj/searchable": "^1.9",
        "pragmarx/tracker": "^3.1",
        "rap2hpoutre/laravel-log-viewer": "^0.10.0",
        "santigarcor/laratrust": "^3.2",
        "spatie/laravel-activitylog": "^1.15",
        "toin0u/geocoder-laravel": "^1.0",
        "venturecraft/revisionable": "^1.28",
        "webpatser/laravel-uuid": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.3",
        "doctrine/dbal": "^2.5",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/lib/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
